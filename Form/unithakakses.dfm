  object Qexec: TMyQuery
    SQLInsert.Strings = (
      'REPLACE INTO hakakses ('
      '  `hak_akses`,'
      '  `akses_laporan`,'
      '  `akses_daftar_pelanggan`,'
      '  `akses_daftar_tunggu`,'
      '  `akses_daftar_rekening`,'
      '  `akses_daftar_sambunganbaru`,'
      '  `akses_daftar_angsuran`,'
      '`akses_daftar_periode`,'
