  object Qpel: TMyQuery
    SQLRefresh.Strings = (
      
        'SELECT p.nosamb,p.nama,p.alamat,COALESCE(c.lembar,0) AS lembar,c' +
        'oalesce(c.rekair,0) as rekair,IF(coalesce(c.lembar,0)>=3,30000,0' +
        ') as denda,18000 as ots,0 as biayalain,0 as uangjaminan FROM pel' +
        'anggan p '
      
        'LEFT JOIN (SELECT nosamb,COUNT(kode) AS lembar,SUm(rekair) AS re' +
        'kair FROM drd WHERE flaglunas="0" AND flagangsur="0" GROUP BY no' +
        'samb ASC) c ON p.nosamb=c.nosamb'
      'WHERE p.status IN("1","2")')
    Connection = Umain.host
    Left = 312
    Top = 248
  end
